@{
    var settlement = (SilverNeedle.Settlements.Settlement)ViewData["settlement"];
    var characters = settlement.Inhabitants;
    ViewData["Title"] = settlement.Name;
    
}

<div class="character-list">
    <h1>
        <span>@settlement.Name</span>
        <a asp-area="" asp-controller="Settlement" asp-action="Settlement" asp-route-strategy="@ViewData["strategy"]" asp-route-number="@ViewData["number"]" class="btn btn-lg btn-primary pull-right" role="button">Regenerate</a>
    </h1>
</div>

