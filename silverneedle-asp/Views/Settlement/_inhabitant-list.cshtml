@{
    var settlement = (SilverNeedle.Settlements.Settlement)ViewData["settlement"];
    var inhabitants = settlement.Inhabitants;
}
<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Inhabitants</h3>
    </div>
    <table class="table table-striped table-bordered statistics">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var person in inhabitants)
            {
                <tr>
                    <td>@person.Name</td>
                </tr>
            }
        </tbody>
    </table>    
</div>